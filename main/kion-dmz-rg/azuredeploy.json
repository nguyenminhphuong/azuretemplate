{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "_artifactsLocation": {
      "type": "string"
    }
  },
  "variables": {
    "sharedresourcesTemplateFolder": "shareresources",
    "vnetTemplateFileName": "vnet.json",
    "vnetTemplateParametersFileName": "sharedresources.parameters.json"
  },
  "resources": [
    {
      "name": "dmzVnetTemplate",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2016-09-01",
      "dependsOn": [],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('_artifactsLocation'), '/', variables('sharedresourcesTemplateFolder'), '/', variables('vnetTemplateFileName'))]",
          "contentVersion": "1.0.0.0"
        },
        "parametersLink": {
          "uri": "[concat(parameters('_artifactsLocation'), '/', variables('sharedresourcesTemplateFolder'), '/', variables('vnetTemplateFileName'))]",
          "contentVersion": "1.0.0.0"
        }
      }
    }],
  "outputs": {}
}
