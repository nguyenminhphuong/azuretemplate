{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnetName": {
      "type": "string"
    },
    "subnetlist": {
      "type": "array"
    }
  },
  "variables": {
    "singleQuote": "'"
  },
  "resources": [
    {
      "copy": {
        "count": "[length(parameters('subnetlist'))]",
        "mode": "Serial",
        "name": "subnetloop"
      },
      "name": "[concat(parameters('vnetName'), '/', parameters('subnetlist')[copyIndex()].name)]",
      "type": "Microsoft.Network/virtualNetworks/subnets",
      "apiVersion": "2017-10-01",
      "location": "[resourceGroup().location]",
      "properties": {
        "addressPrefix": "[parameters('subnetlist')[copyIndex()].addressPrefix]",
        "networkSecurityGroup": {
          "id": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('subnetlist')[copyIndex()].nsgName)]"
        } 
      }
    }
  ],
  "outputs": {
    "testOutput" : {
      "type": "object",
      "value": "[length(parameters('subnetlist')) ]"
    },
    "firstNsg" : {
      "type": "string",
      "value": "[parameters('subnetlist')[1].nsgName]"
    },
    "jsobEval" : {
      "type": "object",
      "value": "[json(concat(variables('singleQuote'),'{\"id\":', resourceId('Microsoft.Network/networkSecurityGroups', 'dmz-public-subnet'), '}', variables('singleQuote') ))]"
    }
  }
}
